{
	"manifest_version": 2,
	"name": "Redmine Extended",
	"description": "A Redmine extension for time track.",
	"version": "1.5",
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [ {
		"css": [ "style.css" ],
		"matches": [ "http://*/*", "https://*/*" ],
		"js": [ "jquery-1.11.1.min.js", "redmine.js" ],
		"run_at": "document_end"
	} ],
	"update_url": "http://clients2.google.com/service/update2/crx"
}
