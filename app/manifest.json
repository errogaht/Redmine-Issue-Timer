{
    "manifest_version": 2,
    "name": "Redmine Issue Timer",
    "description": "A Redmine issue timer.",
    "version": "1.1",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": [
                "styles/main.css"
            ],
            "js": [
                "bower_components/jquery/dist/jquery.min.js",
                "scripts/contentscript.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ]
}