{
  "manifest_version": 2,
  "name": "Redmine Issue Timer",
  "description": "A Redmine extension for timing issues.",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "css": ["styles/main.css"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/jquery-1.11.1.min.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
